     <text:p text:style-name="P63"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc508_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Installing an IDE<text:tab/>5</text:a></text:p>
     <text:p text:style-name="P64"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc510_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Installing Visual Studio Code<text:tab/>5</text:a></text:p>
     <text:p text:style-name="P64"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc512_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Installing Spyder and Jupyter<text:tab/>6</text:a></text:p>
     <text:p text:style-name="P63"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc514_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">hello_world with Visual Studio Code<text:tab/>6</text:a></text:p>
     <text:p text:style-name="P63"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc516_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">hello_world with Spyder<text:tab/>6</text:a></text:p>
     <text:p text:style-name="P63"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc518_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">hello_world with Jupyter Notebook<text:tab/>7</text:a></text:p>
     <text:p text:style-name="P64"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc520_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Updating Anaconda for a specific environment<text:tab/>7</text:a></text:p>
     <text:p text:style-name="P62"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc522_2278608959" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Understanding Python<text:tab/>7</text:a></text:p>
    </text:index-body>
